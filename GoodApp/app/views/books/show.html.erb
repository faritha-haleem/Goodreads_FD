<table>
  <tr>
    <th>Book Name</th>
    <td><%= %></td>
    <th>Publisher Id</th>
    <td><%= %></td>
    <th>ISBN number</th>
  </tr>
  <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= %></td>
        <td><%= book.publisher_id %></td>
        <td><%= %></td>
        <td><%= book.isbn %></td>
      </tr>
  <% end %>
</table>
